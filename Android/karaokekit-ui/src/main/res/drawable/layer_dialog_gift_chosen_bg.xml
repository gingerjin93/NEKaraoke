<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5.6dp" />
            <stroke
                android:width="2dp"
                android:color="#293332" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>